<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

 <class name="stock.vo.User" table="users">
 
  <id column="uid" name="uid" type="string">
  	<generator class="assigned"/>
  </id>
  
  <property column="password" name="password" not-null="true" type="string"/>
  <property column="uname" name="uname" not-null="true" type="string"/>
  <property column="gender" name="gender" not-null="true" type="character"/>
  <property column="birthday" name="birthdate" type="date"/>
  <property column="utype" name="utype" not-null="true" type="string"/>
  <property column="phone" name="phone" type="string"/>
  
 </class>
 
</hibernate-mapping>
